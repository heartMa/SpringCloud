# 3、配置服务的基本信息以及Git仓库的相关信息
spring:
  application:
    name: config-server
  cloud:
    config:
      server:
        git:
          # 访问的git地址
          uri: https://github.com/heartMa/config-repo-demo/
          # 访问Git仓库的用户名
          username: 
          # 访问Git仓库的用户密码
          password: 
# 配置端口号
server:
  port: 1201